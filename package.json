{
  "name": "unleash-vscode",
  "displayName": "Unleash",
  "description": "VS Code extension for Unleash",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:unleash-vscode.helloWorld"
  ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "unleash",
          "title": "Unleash",
          "icon": "resources/unleash_s.svg"
        }
      ]
    },
    "views": {
      "unleash": [
        {
          "id": "unleash",
          "name": "Unleash"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "unleash",
        "contents": "Welcome to the Unleash view! This view is contributed by the `unleash-vscode` extension."
      }
    ],
    "commands": [
      {
        "command": "unleash-vscode.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "yarn run compile-web",
    "vscode:prepublish": "yarn run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/mocha": "^9.1.1",
    "eslint": "^8.20.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4",
    "@vscode/test-web": "^0.0.29",
    "ts-loader": "^9.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "@types/webpack-env": "^1.17.0",
    "assert": "^2.0.0",
    "process": "^0.11.10"
  }
}
